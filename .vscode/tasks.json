{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "build project 555",
            "command": "make",
            "args": [
                "all"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build"
        },
        {
            "type": "shell",
            "label": "clean project 555",
            "command": "make",
            "args": [
                "clean"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build"
        }
    ]
}